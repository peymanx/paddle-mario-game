	pipe= pipe⬇️


	for j=1,max_j do
		for i=1, max_i do

				if j ==1 and i==1 then
							s=16
				elseif j ==1 and i==max_i then
				   s=18
				elseif j ==1 then
							s=17
				elseif j==1 then
						s=32
				else
						s=33
							
				end
				
			
				if j>1 then
				  
