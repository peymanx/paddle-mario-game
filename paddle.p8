function _init()

	starting = true
	frame=true
	layout=1
	
	mapx=0
	mapy=0
	
	--power ups
	powers = {
	   new_ball=12,
	   terminate=13,
	   question=14,
	   coin=15,
	   flower=56,
	   life=21,
	   grow=30,
	   pipe=46,
	   five=47,
	   shrink=31,
	   eframe=44, --enble
	   dframe=45, --disale
	}
	
	coin=0
	life=3
	
	
	--balls
	reset_ball()
		
	--level design
  load_levels()

	

end
function _update()

 if #bricks == 0 then
    layout+=1
    load_levels()
    reset_ball()
 end
 
	if (btnp(❎)) starting=false
	

	if delay >0 then	
		delay-=1
		return
	end
	
	if not starting then	
				for ball in all(balls) do
								ball_update(ball)
				end
	else
					sticky()	
	end
	
	paddle.update()
	
		


end
function _draw()
	cls()
	menu()
	
	

	paddle.draw()
	
	for brick in all(bricks) do
					spr(brick.spr, brick.x, 
					brick.y)
	end
	
	for ball in all(balls) do
					ball_draw(ball)
					
	end


	pipe.draw()	
	
	--spr(1,28,0)
	
	
	
	if frame then
			rect(0,0, 127, 128, 7)
	else
	  rect(0,0, 127, 0, 7)
	end


end


function sticky()
 
	balls={}
	
	local ball = {
				
						x=paddle.x+paddle.w/2-2,
						y=paddle.y- paddle.h/2 -6,
						r=7,
						spr=1,
						ax=0,
						ay=1,
						touch=0
		  }
	add(balls, ball)
end

function reset_ball()
  frame = true
  starting = true
		paddle.x=64-20
		paddle.y=125
		paddle.w = 40
		
		
	balls={}
	
	local ball = {
				
						x=paddle.x+paddle.w/2-2,
						y=paddle.y- paddle.h/2 -6,
						r=7,
						spr=1,
						ax=0,
						ay=1,
						touch=0
		  }
	add(balls, ball)


	delay = 50
	music(0)
	


end
