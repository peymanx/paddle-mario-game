--brick

function collision(a,b,x,y)
		if (a<=x and a+8>=x or 
				x<=a and x+8>=a) and
				(b<=y and b+8>=y or 
				y<=b and y+8>=b) then
				
			  return true
		end
		
		
		return false
end



function coll(x,a)
		if x<=a and x+8>=a then
				
			  return true
		end
		
		
		return false
end


