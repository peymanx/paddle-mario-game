spd = 2.5

function ball_remove(ball)

   del(balls, ball)
			if #balls <= 0 then
			    life-=1
			    
			    reset_ball()
			end
end		
		
function ball_update(ball)
		 hit = false
		 if ball.touch >0 then
		 		ball.touch-=1
		 end
		 ball.spr=1
			ball.x+=ball.ax
			ball.y+=ball.ay
			
			
			
			
			if ball.y<= 1 then

  					hit = true
  					ball.spr=5
  					ball.ay= spd

			end

			if ball.x<= 1 then
			  if frame then
  			  hit = true
  			  ball.spr=4
  					ball.ax= spd
					else
					  ball.x=110
					  
					end
			end
			if ball.x+ball.r>= 127 then
			  if frame then
  					ball.ax= -spd
  					ball.spr=3
  					hit = true
  			else
  			  ball.x=0 			
  			end
			end
			
			if ball.y+ball.r>= 127-paddle.h then
					if paddle.x-4 <= ball.x and
					 paddle.x+paddle.w+4 >= ball.x then
							ball.ay= -spd
							hit= true
							ball.spr=2
							ball.touch=4
					else
					sfx(8)
							ball_remove(ball)
			  end
			
					
			end
			
			

		
			
	for brick in all(bricks) do
			if collision(brick.x, brick.y, 
										ball.x, ball.y) then
			hit=true

			local x = ball.ax		
	  if coll(ball.x, brick.x) then					
					  ball.ax= -spd		
					  ball.ay= spd	  
			elseif coll(brick.x, ball.x) then					
					  ball.ax= spd
					  ball.ay= -spd
			  				
			end		
		
			if coll(ball.y, brick.y) then
					ball.ay= -spd
			elseif coll(brick.y, ball.y) then
					ball.ay= spd
			end
			
			if (x == 0) ball.ax=0 
			
					
			

if brick.spr ==powers.new_ball  then
			sfx(3)
			local ball = {
				
						x=brick.x,
						y=brick.y,
						r=7,
						spr=1,
						ax=0,
						ay=1,
						touch=0
		  }
		  add(balls, ball)
			
end

if brick.spr ==powers.five  then
			sfx(3)
			for i=1,5 do
  			local ball = {
  				
  						x=ball.x,
  						y=ball.y,
  						r=7,
  						spr=1,
  						ax=0,
  						ay=i*0.9,
  						touch=0
  		  }
  		  add(balls, ball)
  		end
			
end

if brick.spr ==powers.life  then
			sfx(9)

			  life+=1
			
end



if brick.spr ==powers.pipe  then
			sfx(11)
   ball.ax = pipe.ax
   ball.ay = pipe.ay
   ball.x = pipe.x
   ball.y = pipe.y
			
end

if brick.spr ==powers.shrink  then
			sfx(10)
   paddle.w-=16
   paddle.x+=8
			
end

if brick.spr ==powers.grow  then
			sfx(10)
   paddle.w+=22
   paddle.x-=11
			
end

if brick.spr ==powers.flower  then
			ball.x-=ball.ax
			ball.y-=ball.ay
			
end

if brick.spr ==powers.eframe  then
			frame = true
			
end

if brick.spr ==powers.dframe  then
			frame = false
			
end



			


			if brick.spr ==powers.coin  then
			sfx(2)

			  coin+=1
			
			end


			if brick.spr ==powers.terminate  then
			sfx(4)

			 ball_remove(ball)
			
			end
			
			if brick.spr ==powers.question  then
				sfx(4)
 
			 brick.spr=rnd_power()
			
			else
			
					del(bricks,brick)
					sfx(0)
			end
			return

			end
	
	end			
			
			
			if hit then
						sfx(0)
			end


		
		
end

function ball_draw(ball)

			spr(ball.spr, ball.x, ball.y)
						
end		


function rnd_power()
  
		r = flr(rnd(10))

		--debug
		--r = 9
		
		
		if (r==1) return powers.life
		if (r==2) return powers.new_ball
		--if (r==3) return powers.terminate
		if (r==4) return powers.grow
		if (r==5) return powers.shrink
		if (r==6) return powers.pipe
		if (r==7) return powers.five

		
		if r==9 then		 
		 if frame then		   
		 		return powers.dframe
		 else
		 		return powers.eframe
		 end
		end
		
		
		return powers.coin

end
