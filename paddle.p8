				if ball.touch>0 then
						ball.ax=-spd
						ball.x-=1
				end

	   end
				

			end
			
			if btn(➡️) then
				paddle.x+=2
				
				for ball in all(balls) do
						if ball.touch>0 then
								ball.ax=spd
								ball.x+=1
						end

	   end

				
				
				

			end

		
		end,
		
		draw=function()
		 rectfill(paddle.x, paddle.y,
		  paddle.x+paddle.w, paddle.y+paddle.h, 
		  paddle.clr)
						
		
		end,


